<Window x:Name="Info_Actividad" x:Class="IPO1_AgenciadeViajes.InfoActividad"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IPO1_AgenciadeViajes"
        mc:Ignorable="d"
        Title="{DynamicResource LblActivida}" Height="848.924" Width="709.2">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="0*"/>
            <RowDefinition Height="106*"/>
            <RowDefinition Height="68*"/>
            <RowDefinition Height="75*"/>
            <RowDefinition Height="77*"/>
            <RowDefinition Height="77*"/>
            <RowDefinition Height="78*"/>
            <RowDefinition Height="80*"/>
            <RowDefinition Height="73*"/>
            <RowDefinition Height="78*"/>
            <RowDefinition Height="64*"/>
            <RowDefinition Height="55*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="35*"/>
            <ColumnDefinition Width="63*"/>
            <ColumnDefinition Width="524*"/>
            <ColumnDefinition Width="88*"/>
        </Grid.ColumnDefinitions>
        <Label x:Name="lblTituloActividad" Content="{DynamicResource lblTituloActividad}" HorizontalAlignment="Left" VerticalAlignment="Top" Height="59" Width="314" Margin="1,29,0,0" FontSize="26" Grid.Column="1" Grid.ColumnSpan="2" Grid.RowSpan="2"/>
        <Label x:Name="lblMonitorActividad" Content="{DynamicResource lblMonitorActividad}" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="1,0,0,0" FontSize="22" Height="43" Width="314" Grid.Column="1" Grid.Row="2" Grid.ColumnSpan="2"/>
        <Label Content="Actividad para realizar en familia." HorizontalAlignment="Left" VerticalAlignment="Bottom" Margin="1,0,0,42" Width="374" FontSize="16" Height="63" Grid.RowSpan="3" Grid.Column="1" Grid.ColumnSpan="2"/>
        <Label x:Name="lblHorarioActividad" Content="{DynamicResource lblHorarioActividad}" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="1,1,0,0" FontSize="22" Height="42" Width="151" Grid.Row="3" Grid.Column="1" Grid.ColumnSpan="2"/>
        <Label Content="De Lunes a Viernes a las 18:00" HorizontalAlignment="Left" VerticalAlignment="Bottom" Margin="2,0,0,0" Width="239" FontSize="16" Height="35" RenderTransformOrigin="0.5,0.5" Grid.Row="3" Grid.Column="1" Grid.ColumnSpan="2">
            <Label.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform AngleX="-0.497"/>
                    <RotateTransform/>
                    <TranslateTransform X="-0.393"/>
                </TransformGroup>
            </Label.RenderTransform>
        </Label>
        <Label x:Name="lblCaracteristicasActividades" Content="{DynamicResource lblCaracteristicasActividades}" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="1,7,0,0" FontSize="22" Height="43" Width="374" Grid.Row="5" Grid.Column="1" Grid.ColumnSpan="2"/>
        <Label Content="Para niños y adultos." HorizontalAlignment="Left" VerticalAlignment="Bottom" Margin="7,0,0,8" Width="375" FontSize="16" Height="33" RenderTransformOrigin="0.5,0.5" Grid.Row="5" Grid.Column="1" Grid.ColumnSpan="2">
            <Label.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform AngleX="-0.497"/>
                    <RotateTransform/>
                    <TranslateTransform X="-0.393"/>
                </TransformGroup>
            </Label.RenderTransform>
        </Label>
        <Label Content="Piscina" HorizontalAlignment="Left" VerticalAlignment="Bottom" Margin="3,0,0,0" Width="358" FontSize="16" Height="41" RenderTransformOrigin="0.5,0.5" Grid.Row="7" Grid.Column="1" Grid.ColumnSpan="2">
            <Label.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform AngleX="-0.497"/>
                    <RotateTransform/>
                    <TranslateTransform X="-0.393"/>
                </TransformGroup>
            </Label.RenderTransform>
        </Label>
        <Button x:Name="btnCancelarActividad" HorizontalAlignment="Left" Margin="342,10,0,0" FontSize="16" Content="{DynamicResource btnCancelarActividad}" Height="52" VerticalAlignment="Top" Width="120" RenderTransformOrigin="0.452,-3.352" Grid.Row="10" Grid.Column="2" />
        <Button x:Name="btnDarActividad" HorizontalAlignment="Left" Margin="4,25,0,0" FontSize="16" Content="{DynamicResource btnDarActividad}" Height="52" VerticalAlignment="Top" Width="166" RenderTransformOrigin="0.452,-3.352" Grid.Row="9" Grid.Column="1" Grid.ColumnSpan="2" />
        <Label Content="Nombre Apellidos" HorizontalAlignment="Left" VerticalAlignment="Bottom" Margin="4,0,0,75" Width="215" FontSize="16" Height="38" RenderTransformOrigin="0.5,0.5" Grid.Row="2" Grid.Column="1" Grid.ColumnSpan="2" Grid.RowSpan="2">
            <Label.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform AngleX="-0.497"/>
                    <RotateTransform/>
                    <TranslateTransform X="-0.393"/>
                </TransformGroup>
            </Label.RenderTransform>
        </Label>
        <Label x:Name="lblCapacidadActividad" Content="{DynamicResource lblCapacidadActividad}" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="1,4,0,0" FontSize="22" Height="41" Width="374" Grid.Row="4" Grid.Column="1" Grid.ColumnSpan="2"/>
        <Label Content="5 - 20 personas" HorizontalAlignment="Left" VerticalAlignment="Bottom" Margin="8,0,0,1" Width="239" FontSize="16" Height="34" RenderTransformOrigin="0.5,0.5" Grid.Row="4" Grid.Column="1" Grid.ColumnSpan="2">
            <Label.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform AngleX="-0.497"/>
                    <RotateTransform/>
                    <TranslateTransform X="-0.393"/>
                </TransformGroup>
            </Label.RenderTransform>
        </Label>
        <Label x:Name="lblPrecioActividad" Content="Precio" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="1,6,0,0" FontSize="22" Height="43" Width="374" Grid.Row="6" Grid.Column="1" Grid.ColumnSpan="2"/>
        <Label Content="5€ la hora." HorizontalAlignment="Left" VerticalAlignment="Bottom" Margin="7,0,0,1" Width="375" FontSize="16" Height="34" RenderTransformOrigin="0.5,0.5" Grid.Row="6" Grid.Column="1" Grid.ColumnSpan="2">
            <Label.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform AngleX="-0.497"/>
                    <RotateTransform/>
                    <TranslateTransform X="-0.393"/>
                </TransformGroup>
            </Label.RenderTransform>
        </Label>
        <Label x:Name="lblAreaActividad" Content="{DynamicResource lblAreaActividad}" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0,6,0,0" FontSize="22" Height="42" Width="564" Grid.Row="7" Grid.Column="1" Grid.ColumnSpan="2"/>
        <Label x:Name="lblEquipamientoActividad" Content="{DynamicResource lblEquipamientoActividad}" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0,7,0,0" FontSize="22" Height="42" Width="564" Grid.Row="8" Grid.Column="1" Grid.ColumnSpan="2"/>
        <Label Content="Chanclas." HorizontalAlignment="Left" VerticalAlignment="Bottom" Margin="3,0,0,71" Width="358" FontSize="16" Height="40" RenderTransformOrigin="0.5,0.5" Grid.Row="8" Grid.Column="1" Grid.RowSpan="2" Grid.ColumnSpan="2">
            <Label.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform AngleX="-0.497"/>
                    <RotateTransform/>
                    <TranslateTransform X="-0.393"/>
                </TransformGroup>
            </Label.RenderTransform>
        </Label>
        <Button x:Name="bntModificarActividad" HorizontalAlignment="Left" Margin="150,25,0,0" FontSize="16" Content="{DynamicResource btnModificarActividad}" Height="52" VerticalAlignment="Top" Width="165" RenderTransformOrigin="0.452,-3.352" Grid.Row="9" Grid.Column="2" />
        <Button x:Name="btnGuardarActividad" HorizontalAlignment="Right" Margin="0,25,10,0" FontSize="16" Content="{DynamicResource btnGuardarActividad}" Height="52" VerticalAlignment="Top" Width="149" RenderTransformOrigin="0.452,-3.352" Grid.Row="9" Grid.Column="2" />
        <Button x:Name="btnInscritosActividad" HorizontalAlignment="Left" Margin="37,11,0,0" FontSize="16" Content="{DynamicResource btnInscritosActividad}" Height="52" VerticalAlignment="Top" Width="262" RenderTransformOrigin="0.452,-3.352" Grid.Row="10" Grid.Column="2" Click="Button_Click" />
        <Label x:Name="lblEstado" Content="{DynamicResource lblEstado}" Margin="0,25,0,0" Grid.Row="11" Background="White" Opacity="0.7" Height="30" Grid.ColumnSpan="4"/>
        <Image x:Name="imgIdioma" HorizontalAlignment="Left" Margin="410,0,0,0" VerticalAlignment="Top" Source="/Recursos/Imagenes/spain.png" Height="39" Width="40" RenderTransformOrigin="0.572,1.906" Grid.Column="2" Grid.RowSpan="2"/>
        <ComboBox x:Name="cbIdiomas" Margin="450,9,25,0" VerticalAlignment="Top" Opacity="0.6" SelectedIndex="0" Background="Black" Height="27" RenderTransformOrigin="0.077,0.142" Grid.Column="2" Grid.RowSpan="2" >
            <ComboBoxItem x:Name="cbItem1" Content="ES"/>
            <ComboBoxItem x:Name="cbItem2" Content="EN"/>
        </ComboBox>
        <Grid HorizontalAlignment="Left" Margin="0,-26,0,15" Width="673" Grid.ColumnSpan="4" Grid.RowSpan="11">
            <Grid.RowDefinitions>
                <RowDefinition Height="123*"/>
                <RowDefinition Height="652*"/>
            </Grid.RowDefinitions>
            <Image HorizontalAlignment="Left" Height="59" Margin="459,63.8,0,0" Grid.Row="1" VerticalAlignment="Top" Width="94"/>
        </Grid>
        <Button x:Name="btnSalir_Copy" ToolTip="Salir" Grid.Column="3" Margin="17,10,40,66" RenderTransformOrigin="0.5,0.707" Click="btnSalir_Click" Grid.RowSpan="2">
            <Image Source="/Recursos/Imagenes/salir_c.png" RenderTransformOrigin="5.078,0.781" Height="30" Width="22" />
        </Button>
    </Grid>
</Window>
